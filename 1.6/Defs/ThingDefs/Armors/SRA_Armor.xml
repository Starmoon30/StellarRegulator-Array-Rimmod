<?xml version="1.0" encoding="utf-8"?>
<Defs>

	<HairDef>
		<defName>SRA_Hidra_Hair</defName>
		<label>Hidra</label>
		<styleGender>Female</styleGender>
		<texPath>SRA/Pawn/Humanlike/Hairs/Hidra_Hair</texPath>
		<category>Misc</category>
		<styleTags>
			<li>SRA_HidraApparel</li>
		</styleTags>
	</HairDef>

	<ThingDef ParentName="ApparelBase">
		<defName>SRA_MetabyssHalo_A</defName>
    <label>诡异光环</label>
    <description>骇人之物正在活动。</description>
	  <useHitPoints>false</useHitPoints>
		<graphicData>
      <texPath>SRA/Pawn/Humanlike/Apparel/SRA_MetabyssHalo</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<shaderType>CutoutComplex</shaderType>
			<onGroundRandomRotateAngle>0</onGroundRandomRotateAngle>
		</graphicData>
	  <destroyOnDrop>true</destroyOnDrop>
    <techLevel>Spacer</techLevel>
		<statBases>
    		 <MarketValue>100</MarketValue>
			<Mass>0.1</Mass>
      		<Flammability>0</Flammability>
			<ArmorRating_Sharp>0</ArmorRating_Sharp>
			<ArmorRating_Blunt>0</ArmorRating_Blunt>
			<ArmorRating_Heat>0</ArmorRating_Heat>
			<EquipDelay>0.1</EquipDelay>
			<Beauty>45</Beauty>
		</statBases>
	  <tradeability>None</tradeability>
	  <generateAllowChance>0</generateAllowChance>
	  <generateCommonality>0</generateCommonality>
		<equippedStatOffsets>
			<PawnBeauty>1</PawnBeauty>
		</equippedStatOffsets>
		<apparel>
			<countsAsClothingForNudity>false</countsAsClothingForNudity>
			<developmentalStageFilter>Adult</developmentalStageFilter>
      <careIfWornByCorpse>false</careIfWornByCorpse>
      <careIfDamaged>false</careIfDamaged>
      <wearPerDay>0</wearPerDay>
			<bodyPartGroups>
				<li>Torso</li>
			</bodyPartGroups>
			<wornGraphicPath>SRA/Pawn/Humanlike/Apparel/SRA_MetabyssHalo</wornGraphicPath>
			<useWornGraphicMask>true</useWornGraphicMask>
			<layers>
				<li>Belt</li>
			</layers>
			<drawData>
				<defaultData>
					<layer>110</layer>
				</defaultData>
			</drawData>
	    <renderSkipFlags>
			<li>None</li>
		  </renderSkipFlags>
		</apparel>
	  <smeltable>false</smeltable>
	  <comps>
	  <li Class="SRA.CompProperties_DamageFactors">
	  <!--白名单为空时不启用-->
			<whitelist>
				<!--<li>Bullet</li>
				<li>RangedStab</li>
				<li>Arrow</li>-->
			</whitelist>
			<damageCap>32</damageCap>
			<candamagedbyRanged>true</candamagedbyRanged>
			<candamagedbynotRanged>false</candamagedbynotRanged>
		</li>
    </comps>
		<colorGenerator Class="ColorGenerator_Options">
			<options>
				<li>
					<weight>10</weight>
					<only>(255,255,255)</only>
				</li>
			</options>
		</colorGenerator>
	</ThingDef>

